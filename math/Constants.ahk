/*
    PI                           --> 3.14159265358979323846264338327950288
    ACos(-1)                     --> 3.14159265358979312
    2*ASin(1)                    --> 3.14159265358979312
    4*ATan(1)                    --> 3.14159265358979312
    16*ATan(1/5) - 4*ATan(1/239) --> 3.14159265358979356
*/
Global PI := ACos(-1)
